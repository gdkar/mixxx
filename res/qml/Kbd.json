    }
    Component.onCompleted: {
        for(var i = 0; i < keyboardConfig.data.length;++i) {
            var sub = keyboardConfig.data[i];
            for(var j = 0; j < sub.data.length; ++j) {
                var ssub = sub.data[j];
                console.log(ssub.group,ssub.item,ssub.seq)
            }
        }
