        PushKey   { item: 'loop_in';seq:'7'}
        PushKey   { item: 'loop_out';seq:'8'}
        PushKey   { item: 'reloop_exit';seq:'9'}

        AddKey    { item: 'rate';amount: -0.5e-2;seq:'F5';min:-3;max:3}
